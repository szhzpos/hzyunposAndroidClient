<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="onFocusTime" format="integer" />
    <declare-styleable name="EditTextForHideKeyBoard">
        <attr name="onFocusTime" />
        <attr name="showSoftKeyboard" format="boolean" />
    </declare-styleable>
    <declare-styleable name="DrawableTextViewTopMargin">
        <attr name="verSpacing" format="dimension" />
        <attr name="bottomSpacing" format="dimension" />
        <attr name="animType" format="integer"/>
        <attr name="selectTextColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="TmpOrderButton">
        <attr name="shape" format="integer"/>
    </declare-styleable>
    <declare-styleable name="ItemPaddingLinearLayout">
        <attr name="ItemPadding" format="dimension"/>
        <attr name="PaddingColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="SwipeLayout">
        <attr name="layout" format="reference" />
    </declare-styleable>
    <declare-styleable name="RoundCornerTabLayout">
        <attr name="borderWidth" format="dimension"/>
        <attr name="color" format="color"/>
    </declare-styleable>

    <declare-styleable name="KeyboardView">
        <attr name="android:keyboardLayout" format="reference"/>
    </declare-styleable>
    <declare-styleable name="JumpTextView">
        <attr name="perId" format="string" />
        <attr name="hide" format="boolean"/>
    </declare-styleable>
</resources>