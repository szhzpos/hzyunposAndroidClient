<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/round_border_sub_gray"
    android:paddingStart="@dimen/margin_8"
    android:paddingTop="@dimen/margin_1"
    android:paddingEnd="@dimen/margin_8"
    android:paddingBottom="@dimen/margin_1">

    <TextView
        android:id="@+id/order_code"
        android:layout_width="0dp"
        android:layout_height="@dimen/height_35"
        android:background="@drawable/bottom_separator"
        android:gravity="center_vertical"
        android:text="N0214-201203030602-0001"
        android:textColor="@color/blue"
        android:textSize="@dimen/font_size_16"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/store_name_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/margin_2"
        android:text="@string/use_store_1"
        android:textColor="@color/text_color"
        android:textSize="@dimen/font_size_16"
        app:layout_constraintStart_toStartOf="@id/order_code"
        app:layout_constraintTop_toBottomOf="@id/order_code" />

    <TextView
        android:id="@+id/store_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/margin_2"
        android:text="总部"
        android:textColor="@color/text_color"
        android:textSize="@dimen/font_size_16"
        app:layout_constraintStart_toEndOf="@id/store_name_label"
        app:layout_constraintTop_toTopOf="@id/store_name_label" />

    <TextView
        android:id="@+id/_order_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/margin_2"
        android:text="2020-12-03 03:06:07"
        android:textColor="@color/text_color"
        android:textSize="@dimen/font_size_16"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/order_code" />

    <TextView
        android:id="@+id/name_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:paddingTop="@dimen/margin_2"
        android:text="@string/time_card_name"
        android:textColor="@color/text_color"
        android:textSize="@dimen/font_size_16"
        app:layout_constraintStart_toStartOf="@id/order_code"
        app:layout_constraintTop_toBottomOf="@id/store_name_label" />

    <TextView
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/margin_2"
        android:text="2020-12-03 03:06:07"
        android:textColor="@color/text_color"
        android:textSize="@dimen/font_size_16"
        app:layout_constraintStart_toEndOf="@id/name_label"
        app:layout_constraintTop_toTopOf="@id/name_label" />

    <TextView
        android:id="@+id/use_time_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/margin_2"
        android:text="@string/use_time"
        android:textColor="@color/text_color"
        android:textSize="@dimen/font_size_16"
        app:layout_constraintStart_toStartOf="@id/_order_time"
        app:layout_constraintTop_toTopOf="@id/name_label" />

    <TextView
        android:id="@+id/use_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/margin_2"
        android:textColor="@color/text_color"
        android:textSize="@dimen/font_size_16"
        app:layout_constraintStart_toEndOf="@id/use_time_label"
        app:layout_constraintTop_toTopOf="@id/use_time_label" />

    <TextView
        android:id="@+id/_vip_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/margin_5"
        android:paddingTop="@dimen/margin_2"
        android:textColor="@color/text_color"
        android:textSize="@dimen/font_size_16"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@id/name_label"
        app:layout_constraintTop_toBottomOf="@id/name_label" />

    <TextView
        android:id="@+id/print"
        android:layout_width="@dimen/width_68"
        android:layout_height="@dimen/height_35"
        android:layout_marginTop="@dimen/margin_5"
        android:layout_marginBottom="@dimen/margin_5"
        android:background="@drawable/button_sub_blue"
        android:gravity="center"
        android:text="@string/m_print_sz"
        android:textColor="@color/white"
        android:textSize="@dimen/font_size_16"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/use_time_label" />

</androidx.constraintlayout.widget.ConstraintLayout>